<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pramesh.bvctimetable">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".BVCTimeTable"
        android:allowBackup="true"
        android:icon="@mipmap/icon_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CseTable"
            android:screenOrientation="landscape">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".CivilTable"
            android:screenOrientation="landscape">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".MechTable"
            android:screenOrientation="landscape">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".EceTable"
            android:screenOrientation="landscape">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".EeeTable"
            android:screenOrientation="landscape">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".YearSelect"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".SemesterSelect"
            android:screenOrientation="portrait" />
        <activity android:name=".SectionSelect" />
        <activity android:name=".CseFirstFirst">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".CseFirstSecond" />
        <activity android:name=".CseSecondFirst" />
        <activity android:name=".CseSecondSecond" />
        <activity android:name=".CseThirdFirst" />
        <activity android:name=".CseFourthFirst" />
        <activity android:name=".CseForthSecond" />
        <activity android:name=".MechFirstFirst" />
        <activity android:name=".MechFirstSecond" />
        <activity android:name=".MechSecondFirst" />
        <activity android:name=".MechSecondSecond" />
        <activity android:name=".MechThirdFirst" />
        <activity android:name=".MechFourthFirst" />
        <activity android:name=".MechFourthSecond"></activity>
    </application>

</manifest>